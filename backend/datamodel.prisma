directive @unique on FIELD | FIELD_DEFINITION

type User {
  id: ID! @unique
  email: String! @unique
  password: String!
  tokens: Int! @default(value: 50)
}

type Course {
  id: ID! @unique
  title: String!
  concepts: [Concept!]!
  order: Int!
}

type Concept {
  id: ID! @unique
  title: String!
  course: Course!
  assessments: [Assessment!]!
  order: Int!
}

type Assessment {
  id: ID! @unique
  concept: Concept!
  assessML: String!
  javaScript: String!
  order: Int!
}