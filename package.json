    "zwitterion": "^0.27.3"
  },
  "scripts": {
    "start": "source ./.development-environment-variables && concurrently \"zwitterion\" \"npm run lambda-proxy\" \"npm run lambda-serve\"",
    "prisma-up": "cd backend && docker-compose up --build",
    "prisma-deploy-development": "source ./.development-environment-variables && cd backend && prisma deploy",
    "prisma-deploy-production": "source /run/media/lastmjs/1A10-AD03/.production-environment-variables && cd backend && prisma deploy",
    "prisma-export": "cd backend && prisma export --path database.zip",
    "prisma-import": "cd backend && prisma import --data database.zip",
    "lambda-proxy": "http-server --cors=Authorization --proxy http://localhost:9000",
    "lambda-build": "netlify-lambda build backend",
    "lambda-serve": "npm run lambda-build && netlify-lambda serve backend --static",
    "build-static-assets": "zwitterion --build-static --exclude node_modules,backend --include @polymer,prendus-question-elements,assessml,prendus-shared,redux/es,@kuscamara,@vaadin",
    "build": "npm run build-static-assets && npm run lambda-build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lastmjs/javascript-practice.git"
